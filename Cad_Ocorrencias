                }
                relatos = relatos.slice(0, -3);
            }

            var envolvidos = '';
            if(document.querySelector('app-dados-grupos').innerHTML.includes('Nenhum Envolvido Informado')) {
                envolvidos = 'Nenhum Envolvido Informado';
