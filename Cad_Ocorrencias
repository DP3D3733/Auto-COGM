setInterval(function() {
    document.querySelectorAll('.tabela-title').forEach(function(item){item.style.display='none'});
    if(localStorage.getItem('selecionaopcao') && localStorage.getItem('selecionaopcao')!= '') {
        if (localStorage.getItem('selecionaopcao').split('-&-').length % 2 != 0) {
            document.querySelector(localStorage.getItem('selecionaopcao').substring(0, localStorage.getItem('selecionaopcao').indexOf('-&-'))).click();
            localStorage.setItem('selecionaopcao', localStorage.getItem('selecionaopcao').replace(localStorage.getItem('selecionaopcao').substring(0, localStorage.getItem('selecionaopcao').indexOf('-&-'))+'-&-',""));
        } else {
            var naotem = 'nao';
            var option = document.querySelectorAll('mat-option');
            for (let i = 0; i < option.length; i++) {
                if(option[i].innerHTML.toUpperCase().includes(localStorage.getItem('selecionaopcao').substring(0, localStorage.getItem('selecionaopcao').indexOf('-&-')).trim().toUpperCase())) {
                    naotem = 'tem';
                    option[i].click();
                    localStorage.setItem('selecionaopcao', localStorage.getItem('selecionaopcao').replace(localStorage.getItem('selecionaopcao').substring(0, localStorage.getItem('selecionaopcao').indexOf('-&-'))+'-&-',""));
                    break;
                }
            };
            if(naotem == 'nao' && document.querySelector('mat-option')) {
                document.querySelector('mat-option').click();
                localStorage.setItem('selecionaopcao', localStorage.getItem('selecionaopcao').replace(localStorage.getItem('selecionaopcao').substring(0, localStorage.getItem('selecionaopcao').indexOf('-&-'))+'-&-',""));
            }
        }
    }
    if(document.querySelector('app-toast')) {
        document.querySelector('app-toast').querySelector('button[aria-label="Fechar Mensagem"]').click();
    }
    if(document.querySelector('app-barra-botoes') && document.querySelector('app-barra-botoes').querySelector('#temos_cads_romu') && localStorage.getItem('cads_romu')) {
        document.querySelector('#resultado_temos_cads_romu').innerHTML = localStorage.getItem('cads_romu');
        document.querySelector('app-barra-botoes').querySelector('iframe').remove();
        localStorage.removeItem('cads_romu');
    }
    async function wait(){
        var cad = await window.navigator.clipboard.readText();
        if(cad.includes('-()-')) {
            if(localStorage.getItem("dados_para_o_cad") && localStorage.getItem("dados_para_o_cad") != cad) {
                localStorage.setItem("dados_para_o_cad",cad)
            } else if(!localStorage.getItem("dados_para_o_cad")) {
                localStorage.setItem("dados_para_o_cad",cad)
            }
        }
    }
    if(document.querySelector('app-barra-botoes') && !document.querySelector('app-barra-botoes').querySelector('#temos_cads_romu')) {
        var a = document.createElement("button");
        document.querySelector("app-barra-botoes").append(a);
        a.innerHTML = 'Criar CAD Rotina';
        a.addEventListener('click',function(){
            wait();
            document.querySelectorAll('div[routerlinkactive="item-menu-selected"]')[1].click();
        });
        a = document.createElement("button");
        a.setAttribute("id", "temos_cads_romu");
        document.querySelector("app-barra-botoes").append(a);
        document.querySelector('#temos_cads_romu').innerHTML = 'Temos CADS';
        a = document.createElement("select");
        a.setAttribute("id", "sel_temos_cads_romu");
        document.querySelector("app-barra-botoes").append(a);
        document.querySelector('#sel_temos_cads_romu').innerHTML = '<option value=romu selected>ROMU?</option><option value=centro>Centro?</option>';
        if(!document.querySelector('#resultado_temos_cads_romu')) {
            a = document.createElement("div");
            a.setAttribute("id", "resultado_temos_cads_romu");
            document.querySelector("app-barra-botoes").append(a);
        }
        document.querySelector('#temos_cads_romu').addEventListener('click',function(){
            let iframe = document.createElement("iframe");
            var today = new Date();
            var dd = String(today.getDate()).padStart(2, '0');
            var mm = String(today.getMonth() + 1).padStart(2, '0'); //January is 0!
            var yyyy = today.getFullYear();
            //https://cadweb.sinesp.gov.br/paineis-estatisticos/app/discover#/?_g=(filters:!(),time:(from:'2024-08-18T09:30:00.000Z',to:now))&_a            =(columns:!(naturezasFinais,nrProtocolo),filters:!(('$state':(store:appState),meta:(alias:!n,disabled:!f,field:empenhos.nomeUnidadeServico,index:cfc8e00b-b566-4aa1-8650-972ed33cc53d,key:empenhos.nomeUnidadeServico,negate:!f,params:(query:romu),                                                    type:phrase),query:(match_phrase:(empenhos.nomeUnidadeServico:romu                                                    ))),('$state':(store:appState),meta:(alias:!n,disabled:!f,index:cfc8e00b-b566-4aa1-8650-972ed33cc53d,key:naturezaInicialDescricao.raw,negate:!t,params:!('Patrulhamento%20Preventivo','Ponto%20Base','A%C3%A7%C3%A3o%20Pr%C3%B3pria','Teste%20de%20Sistema'),type:phrases,value:!('Patrulhamento%20Preventivo','Ponto%20Base','A%C3%A7%C3%A3o%20Pr%C3%B3pria','Teste%20de%20Sistema')),query:(bool:(minimum_should_match:1,should:!((match_phrase:(naturezaInicialDescricao.raw:'Patrulhamento%20Preventivo')),(match_phrase:(naturezaInicialDescricao.raw:'Ponto%20Base')),(match_phrase:(naturezaInicialDescricao.raw:'A%C3%A7%C3%A3o%20Pr%C3%B3pria')),(match_phrase:(naturezaInicialDescricao.raw:'Teste%20de%20Sistema')))))),('$state':(store:appState),meta:(alias:!n,disabled:!f,field:naturezasFinais,index:cfc8e00b-b566-4aa1-8650-972ed33cc53d,key:naturezasFinais,negate:!t,params:!('teste%20de%20sistema'),type:phrases,value:!('teste%20de%20sistema')),query:(bool:(minimum_should_match:1,should:!((match_phrase:(naturezasFinais:'teste%20de%20sistema'))))))),index:cfc8e00b-b566-4aa1-8650-972ed33cc53d,interval:auto,query:(language:kuery,query:'%22naturezasFinais%22:%20*'),sort:!(!(dataHoraRegistroChamadoUTC,desc)))
            iframe.setAttribute('src', "https://cadweb.sinesp.gov.br/paineis-estatisticos/app/discover#/?_g=(filters:!(),time:(from:'"+yyyy+'-'+mm+'-'+dd+"T09:30:00.000Z',to:now))&_a=(columns:!(naturezasFinais,nrProtocolo),filters:!(('$state':(store:appState),meta:(alias:!n,disabled:!f,field:empenhos.nomeUnidadeServico,index:cfc8e00b-b566-4aa1-8650-972ed33cc53d,key:empenhos.nomeUnidadeServico,negate:!f,params:(query:"+document.querySelector('#sel_temos_cads_romu').value+"),type:phrase),query:(match_phrase:(empenhos.nomeUnidadeServico:"+document.querySelector('#sel_temos_cads_romu').value+"))),('$state':(store:appState),meta:(alias:!n,disabled:!f,index:cfc8e00b-b566-4aa1-8650-972ed33cc53d,key:naturezaInicialDescricao.raw,negate:!t,params:!('Patrulhamento%20Preventivo','Ponto%20Base','A%C3%A7%C3%A3o%20Pr%C3%B3pria','Teste%20de%20Sistema'),type:phrases,value:!('Patrulhamento%20Preventivo','Ponto%20Base','A%C3%A7%C3%A3o%20Pr%C3%B3pria','Teste%20de%20Sistema')),query:(bool:(minimum_should_match:1,should:!((match_phrase:(naturezaInicialDescricao.raw:'Patrulhamento%20Preventivo')),(match_phrase:(naturezaInicialDescricao.raw:'Ponto%20Base')),(match_phrase:(naturezaInicialDescricao.raw:'A%C3%A7%C3%A3o%20Pr%C3%B3pria')),(match_phrase:(naturezaInicialDescricao.raw:'Teste%20de%20Sistema')))))),('$state':(store:appState),meta:(alias:!n,disabled:!f,field:naturezasFinais,index:cfc8e00b-b566-4aa1-8650-972ed33cc53d,key:naturezasFinais,negate:!t,params:!('teste%20de%20sistema'),type:phrases,value:!('teste%20de%20sistema')),query:(bool:(minimum_should_match:1,should:!((match_phrase:(naturezasFinais:'teste%20de%20sistema'))))))),index:cfc8e00b-b566-4aa1-8650-972ed33cc53d,interval:auto,query:(language:kuery,query:'%22naturezasFinais%22:%20*'),sort:!(!(dataHoraRegistroChamadoUTC,desc)))");
            iframe.setAttribute('title', 'CADS_ROMU');
            document.querySelector('app-barra-botoes').append(iframe);
        });
    }
    if(document.querySelector('app-inicio-registro-chamada') && !document.body.innerHTML.includes('e suas ocorrências criados com sucesso!')) {
        document.querySelector('app-inicio-registro-chamada').querySelectorAll('div')[3].click();
        document.querySelector("#mat-snack-bar-container-live-1 > div > simple-snack-bar > div.mat-mdc-snack-bar-actions.mdc-snackbar__actions.ng-star-inserted > button")
    }
    if(document.body.innerHTML.includes('e suas ocorrências criados com sucesso!')) {
        document.querySelectorAll('div[routerlinkactive="item-menu-selected"]')[2].click();
        document.querySelector('simple-snack-bar').querySelector('button').click();
    }
     if(document.querySelector('simple-snack-bar') && document.querySelector('app-empenhar-unidade-tela-principal')) {
        document.querySelector('simple-snack-bar').querySelector('button').click();
    }
    if(document.querySelector('app-chamado') && document.querySelector('input[formcontrolname="nomeSolicitante"]') && document.querySelector('input[formcontrolname="nomeSolicitante"]').value == '') {
        document.querySelector('input[formcontrolname="nomeSolicitante"]').value = 'Comando';
        document.querySelector('input[formcontrolname="nomeSolicitante"]').dispatchEvent(new Event('input', { bubbles: true }));
    }
    if(document.querySelector("input[formcontrolname=dataFato") && localStorage.getItem('datahora') && localStorage.getItem('datahora').includes('/')) {
        document.querySelector('input[formcontrolname="dataFato"]').value = localStorage.getItem('datahora').split(' ')[0];
        document.querySelector('input[formcontrolname="dataFato"]').dispatchEvent(new Event('input', { bubbles: true }));
        document.querySelector('input[formcontrolname="horaFato"]').click();
        document.querySelector('input[formcontrolname="horaFato"]').value = localStorage.getItem('datahora').split(' ')[1];
        document.querySelector('input[formcontrolname="horaFato"]').dispatchEvent(new Event('input', { bubbles: true }));
        if(document.querySelector("input[formcontrolname=dataFato").value == localStorage.getItem('datahora').split(' ')[0] && document.querySelector("input[formcontrolname=horaFato").value == localStorage.getItem('datahora').split(' ')[1]) {
            localStorage.removeItem('datahora');
        }
    }
    if(document.querySelector("app-chamado") && document.querySelector("app-chamado").querySelector("input[formcontrolname=pontoReferencia]")) {
        if(localStorage.getItem('ptref') && document.querySelector("app-chamado") && document.querySelector("app-chamado").querySelector("input[formcontrolname=pontoReferencia]").value == '') {
            document.querySelector("app-chamado").querySelector("input[formControlName='pontoReferencia']").value = localStorage.getItem('ptref');
            document.querySelector("app-chamado").querySelector('input[formcontrolname="pontoReferencia"]').dispatchEvent(new Event('input', { bubbles: true }));
        } else if (!localStorage.getItem('ptref') && document.querySelector("input[formControlName='pontoReferencia']").value != ''){
            localStorage.setItem('ptref',document.querySelector("input[formControlName='pontoReferencia']").value);
        }
        if (document.querySelector("div[id='naorepete']")) {
        } else {
            a = document.createElement("div");
            a.setAttribute("id", "naorepete");
            document.querySelector(".espaco").append(a);
            var espaco = document.querySelector(".espaco");
            espaco.innerHTML += '<textarea id=cod_montar_cad></textarea>';
            document.getElementById('cod_montar_cad').addEventListener('input', function(){
                var cod_montar_cad = document.getElementById('cod_montar_cad').value.replaceAll('"',"");
                if(cod_montar_cad.indexOf('Demanda via') > -1) {
                    document.querySelector('input[formcontrolname="nomeSolicitante"]').value = cod_montar_cad.split('*Nome:*')[1].split('\n')[0];
                    document.querySelector('input[formcontrolname="nomeSolicitante"]').dispatchEvent(new Event('input', { bubbles: true }));
                    document.querySelector('input[formcontrolname="nomeSolicitante"]').dispatchEvent(new Event('input', { bubbles: true }));
                    document.querySelector('input[formcontrolname="telefone"]').value = cod_montar_cad.split('*Número:*')[1].split('\n')[0].replace(/[^0-9]/g,'');
                    document.querySelector('input[formcontrolname="telefone"]').dispatchEvent(new Event('input', { bubbles: true }));
                    document.querySelector('input[formcontrolname="telefone"]').dispatchEvent(new Event('input', { bubbles: true }));
                    document.querySelector("textarea[formcontrolname=relato]").value = cod_montar_cad.split('*Situação:*')[1].split('\n')[0];
                    document.querySelector('textarea[formcontrolname="relato"]').dispatchEvent(new Event('input', { bubbles: true }));
                    document.querySelector('textarea[formcontrolname="relato"]').dispatchEvent(new Event('input', { bubbles: true }));
                    document.querySelector('input[placeholder="Selecione o Meio de Aviso"]').value = 'Telefone';
                    document.querySelector('input[placeholder="Selecione o Meio de Aviso"]').dispatchEvent(new Event('input', { bubbles: true }));
                    document.querySelector('input[placeholder="Selecione o Meio de Aviso"]').dispatchEvent(new Event('input', { bubbles: true }));
                    document.querySelector('input[formcontrolname="campoPesquisa"]').value = cod_montar_cad.split('*Endereço:* \n')[1].split('\n')[0].split('http')[0];
                    document.querySelector('input[formcontrolname="campoPesquisa"]').dispatchEvent(new Event('input', { bubbles: true }));
                    document.querySelector('input[formcontrolname="campoPesquisa"]').dispatchEvent(new Event('input', { bubbles: true }));
                    document.querySelector("input[formcontrolname=pontoReferencia]").value = cod_montar_cad.split('*Área de atendimento:* ')[1].split('\n')[0].toUpperCase();
                    document.querySelector('input[formcontrolname="pontoReferencia"]').dispatchEvent(new Event('input', { bubbles: true }));
                    document.querySelector('input[formcontrolname="pontoReferencia"]').dispatchEvent(new Event('input', { bubbles: true }));
                    localStorage.setItem('ptref',document.querySelector("input[formControlName='pontoReferencia']").value);
                    var selecionaopcao = '';
                    if(localStorage.getItem('selecionaopcao')) {
                        selecionaopcao = localStorage.getItem('selecionaopcao');
                    }
                    selecionaopcao += 'input[placeholder="Selecione o Meio de Aviso"]-&-Telefone-&-';
                    if(cod_montar_cad.toUpperCase().indexOf('SAMU') > -1) {
                        selecionaopcao += 'input[placeholder="Selecione a Natureza Inicial"]-&-Apoio ao Samu-&-';
                    }
                    selecionaopcao += 'input[placeholder="Selecione o Tipo de Local"]-&-Residência-&-mat-select[formcontrolname="fatoOcorrendoMomento"]-&-Não-&-';
                    localStorage.setItem('datahora',cod_montar_cad.split('*Data-hora:* ')[1].split('\n')[0]);
                    localStorage.setItem('selecionaopcao',selecionaopcao);
                    document.querySelector('input[placeholder="campoPesquisa"]').focus();
                } else if(cod_montar_cad.indexOf('-()-') > -1 && cod_montar_cad.split('-()-').length == 7) {
                    cod_montar_cad = cod_montar_cad.split('-()-');
                    document.querySelector("input[formcontrolname=pontoReferencia]").value = cod_montar_cad[0];
                    document.querySelector('input[formcontrolname="pontoReferencia"]').dispatchEvent(new Event('input', { bubbles: true }));
                    localStorage.setItem('ptref',document.querySelector("input[formControlName='pontoReferencia']").value);
                    document.querySelector('input[placeholder="Por Favor Informe ao Menos 3 caracteres"]').value = cod_montar_cad[3];
                    document.querySelector('input[placeholder="Por Favor Informe ao Menos 3 caracteres"]').dispatchEvent(new Event('input', { bubbles: true }));
                    selecionaopcao = '';
                    if(localStorage.getItem('selecionaopcao')) {
                        selecionaopcao = localStorage.getItem('selecionaopcao');
                    }
                    selecionaopcao += 'input[placeholder="Selecione o Tipo de Local"]-&-'+cod_montar_cad[1]+'-&-input[placeholder="Selecione a Natureza Inicial"]-&-'+cod_montar_cad[4]+'-&-input[placeholder="Por Favor Informe ao Menos 3 caracteres"]-&-'+cod_montar_cad[3]+'-&-';
                    if(cod_montar_cad[6] && cod_montar_cad[6].includes('/')) {
                        selecionaopcao += 'mat-select[formcontrolname="fatoOcorrendoMomento"]-&-Não-&-';
                        localStorage.setItem('datahora',cod_montar_cad[6]);
                    }
                    localStorage.setItem('selecionaopcao',selecionaopcao);
                    if(cod_montar_cad[5] && cod_montar_cad[5].includes("C") && cod_montar_cad[5].length == 2) {
                        localStorage.setItem('area',"C0"+cod_montar_cad[5].slice(-1));
                    } else {
                        localStorage.setItem('area',cod_montar_cad[5])
                    }
                    document.querySelector("textarea[formcontrolname=relato]").value = cod_montar_cad[2];
                    document.querySelector('textarea[formcontrolname="relato"]').dispatchEvent(new Event('input', { bubbles: true }));
                    //document.querySelector('cad-mapa').querySelector('input').value = cod_montar_cad[3];
                    //document.querySelector('cad-mapa').querySelector('input').dispatchEvent(new Event('input', { bubbles: true }));
                }
            });
            document.querySelector("app-chamado").querySelector("input[formcontrolname=pontoReferencia]").addEventListener('input', function(){
                if(document.querySelector("input[formcontrolname=pontoReferencia]").value.includes('.-.')) {
                    document.querySelector('input[placeholder="Por Favor Informe ao Menos 3 caracteres"]').value = document.querySelector("input[formcontrolname=pontoReferencia]").value.split('.-.')[1].trim();
                    document.querySelector('input[placeholder="Por Favor Informe ao Menos 3 caracteres"]').dispatchEvent(new Event('input', { bubbles: true }));
                    localStorage.setItem('selecionaopcao','input[placeholder="Por Favor Informe ao Menos 3 caracteres"]-&-'+document.querySelector("input[formcontrolname=pontoReferencia]").value.split('.-.')[1].trim()+'-&-input[placeholder="Selecione o Tipo de Local"]-&-'+document.querySelector("input[formcontrolname=pontoReferencia]").value.split('.-.')[2].trim()+'-&-');
                    document.querySelector('input[formcontrolname=pontoReferencia]').value = document.querySelector("input[formcontrolname=pontoReferencia]").value.split('.-.')[0].trim();
                    document.querySelector('input[formcontrolname=pontoReferencia]').dispatchEvent(new Event('input', { bubbles: true }));
                }
                if(document.querySelector("input[formcontrolname=pontoReferencia]").value != '') {
                    localStorage.setItem('ptref',document.querySelector("input[formcontrolname=pontoReferencia]").value);
                }
            });
            document.querySelector("input[formcontrolname=pontoReferencia]").addEventListener('focus', function(){
                document.querySelector("input[formcontrolname=pontoReferencia]").select();
            });
            var datalist = document.createElement("datalist");
            datalist.setAttribute("id", "qth_option");
            datalist.setAttribute("name", "qth_option");
