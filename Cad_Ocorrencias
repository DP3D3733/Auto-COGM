                    span.innerHTML = u[i].innerHTML;
                    if(span.textContent.includes(localStorage.getItem('area').split('-')[0].trim())) {
                        u[i].parentNode.parentNode.parentNode.parentNode.parentNode.parentNode.querySelector('app-botao-acao-finalizar-ocorrencia').querySelector('app-icone-acao-base').querySelector('button').click();
                    }
