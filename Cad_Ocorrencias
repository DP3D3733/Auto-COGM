                area = 'Baltazar';
            }else if(area.trim().substr(0, 1) == '8') {
                area = 'Pinheiro';
            }else if(area.trim().substr(0, 1) == '9') {
                area = 'Sul';
            }else if(area.trim().substr(0, 1) == 'A') {
                area = 'Centro';
